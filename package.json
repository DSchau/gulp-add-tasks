{
  "name": "gulp-add-tasks",
  "version": "1.2.1",
  "description": "A tool to add tasks to a passed-in gulp instance, as a task list, or as a map of functions",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublish": "tsc & npm run changelog",
    "pretest": "tsc --project tsconfig.test.json",
    "test": "mocha",
    "posttest": "del \"test/*.js\" \"src/*.js\""
  },
  "keywords": [
    "gulp",
    "tasks",
    "task list",
    "add"
  ],
  "author": "Dustin Schau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DSchau/gulp-add-tasks"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "conventional-changelog-cli": "^1.2.0",
    "del-cli": "^0.2.0",
    "gulp": "^3.9.1",
    "mocha": "^2.5.3",
    "typescript": "^1.8.10"
  },
  "dependencies": {
    "gulp-help": "^1.6.1",
    "run-sequence": "^1.2.1"
  }
}
